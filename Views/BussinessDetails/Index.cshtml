@model IEnumerable<SimpleApp.Models.BussinessDetails>

@{
    ViewData["Title"] = "Index";
}


<!-- ================ start banner Area ================= -->
<section class="contact-banner-area">
    <div class="container">
        <div class="row justify-content-center align-items-center">
            <div class="col-lg-12 contact-banner-right">
                <h1 class="text-white">
                    Join As Partner
                </h1>
                <p class="mx-auto text-white  mt-20 mb-40">Want to hire a fresher with unmatched technical skills?<br /> Want an affordable, efficient solution to your software problems?</p>
                <p class="mx-auto text-white  mt-20 mb-40" id="hidepara">We offer all that and more in just a click!</p>
                <div class="link-nav">
                    <span class="box">
                        <a href="index.html">Home </a>
                        <i class="lnr lnr-arrow-right"></i>
                        <a href="contacts.html">Contact</a>
                    </span>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- ================ End banner Area ================= -->

<div class="container">
    <h1>Join as Partner</h1>

    <p>
        <a asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.EmailID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MobileNo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Subject)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Message)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.EmailID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MobileNo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Subject)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Message)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.BussinessID">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.BussinessID">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.BussinessID">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>